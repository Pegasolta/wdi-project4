<h1>Felix Admin Page</h1>
<a href="/backend/logout">Logout</a>


<div class="postform">
  <form action="/backend/admin" method="POST">
    <div class="form-group">
      <label for="title">Title</label>
      <input id="postTitle" class="form-control" type="text" name="title">
    </div>

    <div class="form-group">
      <label for="postText">Post Text</label>
      <input id="postText" class="form-control" type="text" name="postText">
    </div>

    <input class="btn btn-primary" type="submit" value="Publish">
  </form>
</div>
